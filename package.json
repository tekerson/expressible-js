{
  "name": "expressible",
  "version": "2.0.0",
  "description": "A library that attempts to make it easier to overcome the expression problem ",
  "main": "expressible/index.js",
  "scripts": {
    "lint": "eslint 'expressible/**/*.js'",
    "test": "tape 'expressible/**/*.spec.js' | faucet",
    "test:watch": "nodemon --quiet --watch 'expressible/' --exec 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tekerson/expressible-js.git"
  },
  "author": "Brenton Alker <brenton@tekerson.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^3.3.0",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "faucet": "0.0.1",
    "nodemon": "^1.10.0",
    "tape": "^4.6.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "new-cap": 0
    }
  }
}
